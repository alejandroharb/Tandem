<div id="{{score.craft}}ScoreModal" class="modal scoreModal">
	<div class="modal-content">
		<h5 class="center-align">Save the hours you've worked on {{score.craft}}!</h5>
			<div class="row">
				<div class="col s8 offset-s2">
					<input id="craft" type="hidden" value="{{score.craft}}">
					<p class="range-field">
						<span><i class="material-icons">query_builder</i></span>
						<input type="range" id="hours{{score.craft}}" min="1" max="10" value="3" />
						<span><i class="material-icons">query_builder</i></span>
					</p>
          <a onClick="saveScores('{{score.craft}}')" class="modal-action modal-close waves-effect waves-green btn-flat theme-color-primary">Enter</a>
				</div>
			</div>
	</div>
</div>
